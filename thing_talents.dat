    <tag group="TalentActive" tag="Active"/>
    <tag group="TalentAction" tag="Incidental"/>
    <tag group="TalentAction" tag="OutOfTurn"/>
    <tag group="Helper" tag="IsRanked"/>
    <tag group="TalentTier" tag="1"/>
