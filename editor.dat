<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- This file contains the definitions of how the Editor should handle all the
      various publicly accessible objects (i.e. component sets) within the game
      system. A separate "editthing" elemenet is defined for each component set,
      along with the appropriate details for how to configure each facet via
      "inputthing" elements.
-->

<document signature="Hero Lab Data">
  <!-- Edit Groups -->
  <inputthinggroup
    id="SourceInfo">
	<inputthing
      name="Source Info"
      helptext="Entries related to the source material are below..">
      <it_separator/>
      </inputthing>
    </inputthinggroup>

	
  <inputthinggroup
    id="ChooseGrp1">
    <inputthing
      name="Selections"
      helptext="User Selections">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Menu #1 Label"
      helptext="Specify the label used to identify choices for display in the first menu. If empty, no first label is shown.">
      <it_field field="usrLabel1"/>
      </inputthing>
    <inputthing
      name="Menu #1 Source"
      helptext="Specify the source from which to retrieve choices for display in the first menu.">
      <it_tagpick group="ChooseSrc1" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #1 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the first menu. If empty, no first menu is shown.">
      <it_field field="usrCandid1"/>
      </inputthing>
    <inputthing
      name="Menu #2 Label"
      helptext="Specify the label used to identify choices for display in the second menu. If empty, no second label is shown.">
      <it_field field="usrLabel2"/>
      </inputthing>
    <inputthing
      name="Menu #2 Source"
      helptext="Specify the source from which to retrieve choices for display in the second menu.">
      <it_tagpick group="ChooseSrc2" tag="?"/>
      </inputthing>
    <inputthing
      name="Menu #2 Tag Expression"
      helptext="Specify the tag expression used to identify choices for display in the second menu. If empty, no second menu is shown.">
      <it_field field="usrCandid2"/>
      </inputthing>
	</inputthinggroup>
	
  <inputthinggroup
    id="BaseGear">
    <inputthing
      name="Rarity"
      helptext="Specify how common the item is (from 1-10) with a lower score being more common.">
      <it_field field="grRarity"/>
      </inputthing>
    <inputthing
      name="Cost"
      helptext="Specify the standard cost of the item, with a value of 0 meaning it is free.">
      <it_field field="grCost"/>
      </inputthing>
    <inputthing
      name="Lot Cost"
      helptext="Specify the cost of a full 'lot' of the item, where the lot size is given on the right. The 'lot cost' overrides any individual cost given above.">
      <it_field field="grLotCost"/>
      </inputthing>
    <inputthing
      name="Encumbrance"
      helptext="Specify the Encumbrance of the item.">
      <it_field field="gearWeight"/>
      </inputthing>
    <inputthing
      name="Hardpoints"
      helptext="Specify the Encumbrance of the item.">
      <it_field field="grHardpoints"/>
      </inputthing>
    </inputthinggroup>

  <inputthinggroup
    id="BaseWeapon">
    <inputthing
      name="Item Attachments Allowed"
      helptext="Specify any item attachment categories allowed.">
      <it_taglist group="ItemModCatAllowed" tag="Weapon?"/>
      </inputthing>
    <inputthing
      name="Weapon Bonus"
      helptext="Specify the attack bonus of the weapon.">
      <it_field field="wpBonus"/>
      </inputthing>
    <inputthing
      name="Weapon Damage"
      helptext="Specify the damage rating of the weapon.">
      <it_field field="wpDamage"/>
      </inputthing>
    <inputthing
      name="Weapon Crit Rating"
      helptext="Specify the crit rating of the weapon.">
      <it_field field="wpCrit"/>
      </inputthing>
    <inputthing
      name="Weapon Range"
      helptext="Specify what range this weapon has. Melee weapons are typically Engaged.">
      <it_taglist group="WeaponRange" tag="?"/>
      </inputthing>
    <inputthing
      name="Minimum Strength"
      helptext="Specify the minimum Strength required to use the weapon without penalty.">
      <it_field field="wpStrReq"/>
      </inputthing>
    <inputthing
      name="Minimum Agility"
      helptext="Specify the minimum Agility required to use the weapon without penalty.">
      <it_field field="wpAgiReq"/>
      </inputthing>
    <inputthing
      name="Item Qualities"
      helptext="Specify any item qualities of the weapon.">
      <it_taglist group="ItemQuality" tag="?"/>
      </inputthing>
    <inputthing
      name="Special Behaviors"
      helptext="Specify any special behaviors for the weapon of which the user should be reminded.">
      <it_field field="wpSpecial"/>
      </inputthing>
    <inputthing
      name="Hide from user?"
      helptext="Specify whether this weapon is shown when choosing weapons.">
      <it_tagcheck group="Hide" tag="Equipment"/>
      </inputthing>
    <inputthing
      name="Natural Attack?"
      helptext="Specify whether this weapon should be treated as a natural attack.">
      <it_tagcheck group="Equipment" tag="Natural"/>
      </inputthing>
    <inputthing
      name="Two-Handed Weapon?"
      helptext="Specify whether this weapon is two-handed.">
      <it_tagcheck group="Equipment" tag="TwoHand"/>
      </inputthing>
    </inputthinggroup>
	
  <inputthinggroup
    id="BaseDefense">
    <inputthing
      name="Defense rating"
      helptext="Defense Values">
      <it_separator/>
      </inputthing>	 
    <inputthing
      name="Defense Rating"
      helptext="Specify the Defense rating of the armor against attacks.">
      <it_field field="defDefense"/>
      </inputthing>
    <inputthing
      name="Soak Rating"
      helptext="Specify the Soak rating of the armor against attacks.">
      <it_field field="defSoak"/>
      </inputthing>
    <inputthing
      name="Minimum Strength"
      helptext="Specify the minimum Strength required to use the armor successfully.">
      <it_field field="defStrReq"/>
      </inputthing>

    </inputthinggroup>	

	
  <!-- Attribute editthing -->
  <editthing
    compset="Attribute"
    name="Attribute"
    prefix="attr"
    description="Attributes are incredibly simple. They have a minimum and maximum value.{br}They also have an optional short name and/or abbreviation."
    summary="Defines an Attribute upon which starting XPs can be spent by a character.">
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the attribute (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the attribute (default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the attribute via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
    </editthing>


  <!-- Skill editthing -->
  <editthing
    compset="Skill"
    name="Skill"
    prefix="sk"
    description="As basic traits, Skills have a minimum and maximum value, as well as{br}an optional short name and/or abbreviation."
    summary="Defines a Skill upon which starting XPs can be spent by a character.">
	
    <inputthing
      name="Category"
      helptext="Select the category this Skill belongs to.">
      <it_taglist group="SkillCat" tag="?"/>
      </inputthing>
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the skill (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the skill(default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the skill via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
    <inputthing
      name="Linked Attribute for Skill"
      helptext="Select the attribute used to calculate the dice pool for this skill.">
      <it_linkage compset="Attribute" linkage="attribute"/>
      </inputthing>
    </editthing>


  <!-- Ability editthing -->
  <editthing
    compset="Ability"
    name="Abilities"
    prefix="ab"
    description="As basic traits, special abilities have various characteristics that are not currently used.{br}They also have an optional custom name for display on the Special tab."
    summary="Defines a Special Ability upon which starting XPs can be spent by a character.">
    <inputthing
      name="Name for Special Tab"
      helptext="Specify an optional custom name to be used on the Special tab.">
      <it_field field="spcName"/>
      </inputthing>
    <inputthing
      name="Hide from User?"
      helptext="Specify whether this special ability is hidden from direct user selection (e.g. Flight is only conferred by the Avian race).">
      <it_tagcheck group="Hide" tag="Ability"/>
      </inputthing>
    <inputthing
      name="Activated by User?"
      helptext="Specify whether this special ability can be activated by the user via the In-Play tab.">
      <it_tagcheck group="User" tag="Activation"/>
      </inputthing>
    <inputthing
      name="Creation Only?"
      helptext="Specify whether this special ability can only be selected for a character during creation.">
      <it_tagcheck group="User" tag="CreateOnly"/>
      </inputthing>

    <inputthing>
      <it_groupref id="ChooseGrp1"/>
      </inputthing>	
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
<!-- These facets of special abilities are not currently used for anything so they are commented out.
    <inputthing
      name="Minimum Value"
      helptext="Specify the minimum rating for the special ability (default is 0).">
      <it_field field="trtMinimum"/>
      </inputthing>
    <inputthing
      name="Maximum Value"
      helptext="Specify the maximum rating for the skill(default is 10).">
      <it_field field="trtMaximum"/>
      </inputthing>
    <inputthing
      name="Abbreviation"
      helptext="Specify a very short abbreviation for use in displaying the skill via its abbreviation (max 5 characters).">
      <it_field field="trtAbbrev"/>
      </inputthing>
-->
    </editthing>

  <!-- Archetype editthing -->
  <editthing
    compset="Career"
    name="Career"
    prefix="ca"
    description="Careers have no special fields - only a name. If a race confers an ability,{br}create it as a special ability and bootstrap it from the race."
    summary="Defines a career that can be selected by a character.">

    <inputthing
      name="Career Skill selections"
      helptext="Number of skills with free ranks for career.">
      <it_field field="caSkillChoices"/>
      </inputthing> 
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
	  
    </editthing>
	
  <!-- Archetype editthing -->
  <editthing
    compset="Archetype"
    name="Archetype"
    prefix="arc"
    description="Archetypes have no special fields - only a name. If a race confers an ability,{br}create it as a special ability and bootstrap it from the race."
    summary="Defines a Archetype that can be selected by a character.">

    <inputthing
      name="Starting Attributes"
      helptext="Beginning attribute values.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Brawn">
      <it_field field="arBra"/>
      </inputthing>
    <inputthing
      name="Agility">
      <it_field field="arAgi"/>
      </inputthing>
    <inputthing
      name="Intellect">
      <it_field field="arInt"/>
      </inputthing>
    <inputthing
      name="Cunning">
      <it_field field="arCun"/>
      </inputthing>
    <inputthing
      name="Willpower">
      <it_field field="arWil"/>
      </inputthing>
    <inputthing
      name="Presence">
      <it_field field="arPre"/>
      </inputthing>
    <inputthing
      name="Starting Values"
      helptext="Beginning values for other fields">
      <it_separator/>
      </inputthing>	 
    <inputthing
      name="Wound Threshold"
      helptext="Wound Threshold is this + Brawn.">
      <it_field field="arWound"/>
      </inputthing>	 
    <inputthing
      name="Strain Threshold"
      helptext="Strain Threshold is this + Willpower.">
      <it_field field="arStrain"/>
      </inputthing>
    <inputthing
      name="Starting XP">
      <it_field field="arXP"/>
      </inputthing>	
	  
    <inputthing>
      <it_groupref id="ChooseGrp1"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
	  
    </editthing>


  <!-- Equipment editthing - for basic gear -->
  <editthing
    compset="Equipment"
    name="Equipment"
    prefix="eq"
	defchild="ItemMods"
    description="Weapons, armor, and vehicles all have their own tabs for creating objects of those types."
    summary="Defines a simple piece of equipment that can be selected on the Gear tab.">

    <inputthing
      name="Container Behavior"
      helptext="Specify whether this piece of equipment can contain other equipment. If this equipment can contain other equipment AND be contained within other equipment, select 'Holder'. If the equipment can only contain other equipment, select 'Top Holder'. If the equipment can only be contained by other equipment, do not select anything.">
      <it_tagpick group="thing" tag="holder?" />
      </inputthing>
    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Ammunition editthing - for ammunition -->
  <editthing
    compset="Ammunition"
    name="Ammunition"
    prefix="eq"
	defchild="ItemMods"
    description="Weapons, armor, and vehicles all have their own tabs for creating objects of those types."
    summary="Defines a type of weapon ammunition that can be selected on the Gear tab.">

    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Melee editthing - for melee weapons -->
  <editthing
    compset="Melee"
    name="Melee Weapon"
    prefix="wp"
	defchild="ItemMods"
    description="Armor, vehicles, and simple equipment all have their own tabs for creating objects of those types."
    summary="Defines a melee weapon that can be selected on the Armory tab.">
    <inputthing
      name="Weapon Skill"
      helptext="Specify what skill is used for this weapon.">
      <it_taglist group="WeaponSkill" tag="skMel?"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseWeapon"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Ranged editthing - for ranged weapons -->
  <editthing
    compset="Ranged"
    name="Ranged Weapon"
    prefix="wp"
	defchild="ItemMods"
    description="Armor, vehicles, and simple equipment all have their own tabs for creating objects of those types."
    summary="Defines a ranged weapon that can be selected on the Armory tab.">
    <inputthing
      name="Weapon Skill"
      helptext="Specify what skill is used for this weapon.">
      <it_taglist group="WeaponSkill" tag="skRange?"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseWeapon"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Armor editthing -->
  <editthing
    compset="Armor"
    name="Armor"
    prefix="arm"
	defchild="ItemMods"
    description="Weapons, vehicles, and simple equipment all have their own tabs for creating objects of those types."
    summary="Defines armor that can be selected on the Armory tab.">
    <inputthing
      name="Item Attachments Allowed"
      helptext="Specify any item attachment categories allowed.">
      <it_taglist group="ItemModCatAllowed" tag="Armor?"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseDefense"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Shield editthing -->
  <editthing
    compset="Shield"
    name="Shield"
    prefix="shd"
	defchild="ItemMods"
    description="Weapons, vehicles, and simple equipment all have their own tabs for creating objects of those types."
    summary="Defines a shield that can be selected on the Armory tab.">
    <inputthing>
      <it_groupref id="BaseDefense"/>
      </inputthing>
    <inputthing>
      <it_groupref id="BaseGear"/>
      </inputthing>
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>


  <!-- Mechanics editthing -->
  <editthing
    compset="Mechanics"
    name="Mechanic"
    description="A mechanics thing will be automatically added to every character to apply its effects. There is no built-in logic for mechanics,{br}as they provide a means for the user to customize everything to work as he chooses. As such, there are no fields and{br}everything is assumed to be done through scripts."
    summary="Defines a set of custom behaviors that are defined by the user and applied to each character.">
    <inputthing
      name="Optional Rules"
      helptext="Optional rules in place for a setting.">
      <it_separator/>
      </inputthing>
    <inputthing
      name="Use Item Atachments/Hardpoints?"
      helptext="Specify whether this setting uses Hardpoints and allows for Item Modifications.">
      <it_tagcheck group="Hero" tag="UseItemMods"/>
      </inputthing> 
	  
    <inputthing
      name="Setting Rules"
      helptext="Various rules in place for a setting.">
      <it_separator/>
      </inputthing>	 
    <inputthing
      name="Split Melee Skill?"
      helptext="Specify whether this setting uses Light/Heavy versions of the Melee skill.">
      <it_tagcheck group="SettingRule" tag="SplitMelee"/>
      </inputthing> 
    <inputthing
      name="Split Ranged Skill?"
      helptext="Specify whether this setting uses Light/Heavy versions of the Ranged skill.">
      <it_tagcheck group="SettingRule" tag="SplitRanged"/>
      </inputthing> 
    <inputthing
      name="Magic in Use?"
      helptext="Specify whether this setting uses Magic based skills.">
      <it_tagcheck group="SettingRule" tag="UseMagic"/>
      </inputthing>
    </editthing>

  <!-- Talent editthing -->
  <editthing
    compset="Talent"
    name="Talents"
    prefix="tal"
    description="Talents for purchase"
    summary="Defines a Talent for purchase by a hero.">
    <inputthing
      name="Ranked?"
      helptext="Specify whether this talent is ranked.">
      <it_tagcheck group="Helper" tag="IsRanked"/>
      </inputthing>
    <inputthing
      name="Talent Tier"
      helptext="Specify what tier this tag belongs to.">
      <it_tagpick group="TalentTier" tag="?" require="yes" deftag="1"/>
      </inputthing>
    <inputthing
      name="Activation"
      helptext="Specify what type of Activation this talent has.">
      <it_tagpick group="TalentActive" tag="?" require="yes" deftag="Active"/>
      </inputthing>
    <inputthing
      name="Action"
      helptext="Specify what type of Action(s) this talent has.">
      <it_taglist group="TalentAction" tag="?" />
      </inputthing>
	  
    <inputthing>
      <it_groupref id="ChooseGrp1"/>
      </inputthing>	  
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
    </editthing>
	
  <!-- Equipment editthing - for basic gear -->
  <editthing
    compset="HeroicAbility"
    name="Heroic Ability"
    prefix="ha"
    description="Heroic abilities."
    summary="Defines a simple ability that can be selected.">

    <inputthing>
      <it_groupref id="ChooseGrp1"/>
      </inputthing>	
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
	  
    </editthing>
	
  <!-- Material editthing - for use with magic Implements-->
  <editthing
    compset="Material"
    name="Material"
    prefix="mat"
    description="Material for use with magic implements"
    summary="Magical Implement material.">

    <inputthing
      name="Cost Multiplier">
      <it_field field="matPriceMod"/>
      </inputthing>	
    <inputthing
      name="Rarity Modifier">
      <it_field field="matRarityMod"/>
      </inputthing>	
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
	  
    </editthing>
	
  <!-- Material editthing - for use with magic Implements-->
  <editthing
    compset="Construction"
    name="Construction Type"
    prefix="ct"
    description="Construction for Armor and Weapons"
    summary="Armor/Weapon Construction Type.">

    <inputthing
      name="Type"
      helptext="Specify what type of  item this is for (Armor/Weapon).">
      <it_taglist group="ConType" tag="?" />
      </inputthing>
    <inputthing
      name="Cost Multiplier">
      <it_field field="ctPriceMod"/>
      </inputthing>	
    <inputthing
      name="Rarity Modifier">
      <it_field field="ctRarityMod"/>
      </inputthing>	
    <inputthing>
      <it_groupref id="SourceInfo"/>
      </inputthing>
	  
    </editthing>
	
  <!-- ItemMods editthing -->
  <editthing
    compset="ItemMod"
    name="Item Attachments"
    prefix="imod"
    description="Abilities defined as Item Modifications."
    summary="Defines Item Modifications that can be selected through the item mod gizmo.">

    <inputthing
      name="Hardpoints">
      <it_field field="imodHPCost"/>
      </inputthing>
    <inputthing
      name="Cost">
      <it_field field="imodCost"/>
      </inputthing>
    <inputthing
      name="Rarity"
      helptext="Specify how common the item is (from 1-10) with a lower score being more common.">
      <it_field field="imodRarity"/>
      </inputthing>
    <inputthing
      name="Mod Category"
      helptext="Specify what type of Item Mod this is for.">
      <it_taglist group="ItemModCat" tag="?"/>
      </inputthing>
    <inputthing
      name="Weapon Effect?"
      helptext="Specify what effect this is mod adds to a weapon.">
      <it_taglist group="Weapon" tag="?"/>
      </inputthing>
	  
    </editthing>
  </document>
